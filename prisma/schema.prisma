// Prisma schema file
// Created for user table with webauthn fields

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id           String @id @default(cuid())
  phoneNumber  String @unique
  credentialId Bytes?
  publicKey    Bytes?
  counter      Int?
}
